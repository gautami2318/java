import java.util.Scanner;
class add
{
  public static void main(String []args)
  {
    int a,b,sum;
    System.out.println("enter first number: ");
    Scanner sc=new Scanner(System.in);
    a=sc.nextInt();
    System.out.println("enter second number: ");
    b=sc.nextInt();
    sum=a+b;
    System.out.println("Addition :"+sum);
  }
}
